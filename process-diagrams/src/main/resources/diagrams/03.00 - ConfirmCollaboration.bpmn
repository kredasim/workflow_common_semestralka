      <lane id="studentLane" name="Student">
        <flowNodeRef>startevent1</flowNodeRef>
        <flowNodeRef>usertask1</flowNodeRef>
        <flowNodeRef>exclusivegateway1</flowNodeRef>
        <flowNodeRef>scripttask1</flowNodeRef>
        <flowNodeRef>scripttask2</flowNodeRef>
        <flowNodeRef>endeventConfirmed</flowNodeRef>
        <flowNodeRef>endeventCanceled</flowNodeRef>
      </lane>
    </laneSet>
    <startEvent id="startevent1" name="Start"></startEvent>
    <userTask id="usertask1" name="01 - Confirm collaboration"></userTask>
    <boundaryEvent id="boundarysignal" name="" cancelActivity="true" attachedToRef="usertask1">
      <signalEventDefinition signalRef="collaborationInitiatedSignal"></signalEventDefinition>
    </boundaryEvent>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway" default="flow3"></exclusiveGateway>
    <scriptTask id="scripttask1" name="Remove from candidates" scriptFormat="groovy">
      <script><![CDATA[System.out.println("Removing from candidates");]]></script>
    </scriptTask>
    <scriptTask id="scripttask2" name="Initiate cooperation" scriptFormat="groovy">
      <script><![CDATA[System.out.println("Initiating cooperation");]]></script>
    </scriptTask>
    <endEvent id="endeventConfirmed" name="Confirmed"></endEvent>
    <endEvent id="endeventCanceled" name="Canceled"></endEvent>
    <sequenceFlow id="flow1" name="" sourceRef="startevent1" targetRef="usertask1"></sequenceFlow>
    <sequenceFlow id="flow2" name="" sourceRef="usertask1" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow3" name="Rejected" sourceRef="exclusivegateway1" targetRef="scripttask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${exclusiveGatewayDesicion == "rejected"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow4" name="Confirmed" sourceRef="exclusivegateway1" targetRef="scripttask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${exclusiveGatewayDesicion == "confirmed"}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow5" name="" sourceRef="scripttask1" targetRef="endeventCanceled"></sequenceFlow>
    <sequenceFlow id="flow6" name="" sourceRef="scripttask2" targetRef="endeventConfirmed"></sequenceFlow>
    <sequenceFlow id="flow7" name="" sourceRef="boundarysignal" targetRef="endeventCanceled"></sequenceFlow>
  </process>
